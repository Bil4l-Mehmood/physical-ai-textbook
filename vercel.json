{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
 "installCommand": "npm install --legacy-peer-deps",
  "framework": "docusaurus",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]

  ,
  "build": {
    "env": {
      "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
    }
  }


  
}


